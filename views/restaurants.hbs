<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<p>{{Test}}</p>

  <div class="text-center grid-container align-center">
    <div class="grid-block home-img-bg">
      {{#each restaurants}}
      <div class="grid-container v-align">
        <img href="{{featured_image}}"></img>
        <h1>{{name}}</h1>
        <p>$ {{average_cost_for_two}}.00-ish per person incl. drinks</p>
        <p>{{user_rating.aggregate_rating}} out of 5. </p>
        <p>{{user_rating.rating_text}} based on {{user_rating.votes}} votes</p>
        <p>{{cuisines}}</p>
      </div>
      {{/each}}
    <div class="grid-block"></div>
    <div class="grid-container"></div>
    <div class="footer"></div>
  </div>
